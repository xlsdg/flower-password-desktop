{
  "name": "FlowerPassword",
  "productName": "FlowerPassword",
  "version": "3.6.4",
  "description": "flower password electron app",
  "main": "dist/main/main.js",
  "scripts": {
    "build": "rspack build -c rspack.config.js",
    "watch": "rspack build -c rspack.config.js --watch",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md,css,less,html}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,json,md,css,less,html}\"",
    "start": "npm run build && electron-forge start",
    "dev": "concurrently \"npm run watch\" \"nodemon\"",
    "package": "npm run build && electron-forge package",
    "make": "npm run build && electron-forge make --arch=arm64",
    "make:x64": "npm run build && electron-forge make --arch=x64",
    "make:all": "npm run make && npm run make:x64",
    "make:mac": "npm run build && electron-forge make --platform=darwin",
    "make:mac:arm64": "npm run build && electron-forge make --platform=darwin --arch=arm64",
    "make:mac:x64": "npm run build && electron-forge make --platform=darwin --arch=x64",
    "make:win": "npm run build && electron-forge make --platform=win32",
    "make:win:x64": "npm run build && electron-forge make --platform=win32 --arch=x64",
    "make:win:ia32": "npm run build && electron-forge make --platform=win32 --arch=ia32",
    "make:linux": "npm run build && electron-forge make --platform=linux",
    "make:linux:x64": "npm run build && electron-forge make --platform=linux --arch=x64",
    "make:linux:arm64": "npm run build && electron-forge make --platform=linux --arch=arm64"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xlsdg/flower-password-desktop.git"
  },
  "author": "xLsDg",
  "license": "MIT",
  "dependencies": {
    "flowerpassword.js": "^5.0.2",
    "i18next": "^25.6.0",
    "psl": "^1.15.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@rspack/cli": "^1.5.8",
    "@rspack/core": "^1.5.8",
    "@types/node": "^22.18.11",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "concurrently": "^9.2.1",
    "css-loader": "^7.1.2",
    "electron": "^38.3.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "html-rspack-plugin": "^6.1.3",
    "less": "^4.4.2",
    "less-loader": "^12.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "style-loader": "^4.0.0",
    "typescript": "^5.9.3"
  }
}
