{
  "name": "FlowerPassword",
  "productName": "FlowerPassword",
  "version": "4.1.0",
  "description": "flower password electron app",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make --arch=arm64",
    "make:x64": "electron-forge make --arch=x64",
    "make:all": "npm run make && npm run make:x64",
    "make:mac": "electron-forge make --platform=darwin",
    "make:mac:arm64": "electron-forge make --platform=darwin --arch=arm64",
    "make:mac:x64": "electron-forge make --platform=darwin --arch=x64",
    "make:win": "electron-forge make --platform=win32",
    "make:win:x64": "electron-forge make --platform=win32 --arch=x64",
    "make:win:ia32": "electron-forge make --platform=win32 --arch=ia32",
    "make:linux": "electron-forge make --platform=linux",
    "make:linux:x64": "electron-forge make --platform=linux --arch=x64",
    "make:linux:arm64": "electron-forge make --platform=linux --arch=arm64",
    "lint": "eslint --fix .",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md,css,less,html}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,json,md,css,less,html}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xlsdg/flower-password-desktop.git"
  },
  "author": "xLsDg",
  "license": "MIT",
  "dependencies": {
    "auto-launch": "^5.0.6",
    "flowerpassword.js": "^5.0.2",
    "i18next": "^25.7.3",
    "psl": "^1.15.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/plugin-vite": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@types/node": "^22.19.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-react": "^5.1.2",
    "electron": "^39.2.7",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "less": "^4.5.1",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  }
}
